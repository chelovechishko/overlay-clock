{
   "browser_action": {
      "default_icon": {
         "19": "clock-icon19.png",
         "38": "clock-icon38.png"
      },
      "default_popup": "options.html",
      "default_title": "Overlay Clock"
   },
   "content_scripts": [ {
      "js": [ "defaults.js", "clock.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "default-src 'none'; style-src 'self'; script-src 'self';",
   "description": "Displays a digital clock at the corner of the page.",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "Overlay Clock",
   "options_page": "options.html",
   "permissions": [ "storage" ],
   "short_name": "OverlayClock",
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "0.1"
}
